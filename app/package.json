    "@scalar/api-reference": "^1.24.11",
    "vue": "^3.4.21",
    "vue-cookies": "^1.8.4",
    "vue-router": "^4.3.3",
    "vuetify": "^3.6.9"
  },
  "devDependencies": {
    "@mdi/font": "^7.4.47",
    "@rushstack/eslint-patch": "^1.10.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.14.5",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitest/coverage-v8": "^1.6.0",
